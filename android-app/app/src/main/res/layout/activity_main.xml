<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="uqu.com.sampleapp.MainActivity"
    android:gravity="center">

    <Button
        android:id="@+id/mapButton"
        android:text="Map Activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/mapButton2"
        android:text="Map Activity-SubClass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button

        android:id="@+id/routeToButton"
        android:text="RouteToActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <Button

        android:id="@+id/filterButton"
        android:text="FilterActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />



    <Button

        android:id="@+id/aboutButton"
        android:text="AboutActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />



</LinearLayout>
